<addons>
<addon id="plugin.rutracker" name="RuTracker" version="0.20.4mod28" provider-name="HAL9000">
  <requires>
    <import addon="xbmc.python" version="2.14.0"/>
    <import addon="script.module.requests"/>
    <import addon="script.module.delugestream" version="0.1.1" optional="true"/>
    <import addon="script.module.torrent.ts" optional="true" />
    <import addon="script.module.libtorrent" optional="true" />
    <import addon="script.service.torrserver" optional="true" />
    <import addon="script.module.torrserver" optional="true" />
    <import addon="plugin.video.elementum" optional="true" />
    <import addon="plugin.video.torrenter" optional="true" />
    <import addon="plugin.video.tam" optional="true" />
  </requires>
  <extension point="xbmc.python.pluginsource" provides="video" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <news>Добавлены "Новинки ..." во всех подразделах</news>
    <summary lang="en">RuTracker</summary>
    <description lang="en">You can search RuTracker and download torrents.</description>
    <summary lang="ru">Навигатор по RuTracker</summary>
    <description lang="ru">Навигатор по сайту RuTracker с возможностью скачивания торрентов. Просмотр через Elementum, TorrServer, Torrenter, TAM, Libtorrent, TorrentStream, DelugeStream. Скачивание торрентов с помощью Transmission и uTorrent.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="script.module.tengine" name="TEngine" version="1.5.9mod" provider-name="1orgar">
  <requires>
    <import addon="xbmc.python" version="2.7.13"/>
    <import addon="script.module.libtorrent"/>
    <import addon="script.module.torrent.ts"/>
    <import addon="script.module.pyrrent2http"/>
    <import addon="script.module.torrent2http"/>
    <import addon="script.module.cdebug"/>
  </requires>
  <extension point="xbmc.python.script" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.python.module" library="lib" />
  <extension point="xbmc.addon.metadata">
    <summary>TEngine torrent streaming support</summary>
    <description>Simple interface to most popular KODI torrent streaming engines</description>
    <license>GPL</license>
    <platform>all</platform>
  </extension>
</addon>

